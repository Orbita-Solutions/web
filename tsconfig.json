{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@core/*": ["src/core/*"],
      "@layouts/*": ["src/layouts/*"],
      "@hooks/*": ["src/hooks/*"],
      "@svgs/*": ["src/svgs/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
